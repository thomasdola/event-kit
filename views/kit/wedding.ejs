<body>
<div class="ui vertical center aligned segment">
    <div class="ui container">
        <div class="ui large secondary pointing menu">
            <a class="item">
                <img src="/images/logo.png" alt="company-logo">
            </a>

            <div class="right menu">
                <a class="ui item" href="/#About-Us">
                    About Us
                </a>
                <a class="ui item" href="/#Services">
                    Services
                </a>
                <a class="ui item" href="/#Partners">
                    Partners
                </a>
                <a class="ui item" href="/#Contact-Us">
                    Contact Us
                </a>
            </div>
        </div>
    </div>
</div>


<div class="ui container main-content">

    <!--box header-->
    <div class="ui menu box-header">
        <div class="item link Review_Button">
            <i class="shop icon"></i>
        </div>
        <div class="right menu">
            <div data-tooltip="doubleclick budget to change amount.">
                <div class="item link __change_budget">
                    Budget: <span>&#8373; <b>20,000.00</b></span>
                </div>
            </div>

            <div class="item">
                Balance: <span>&#8373; <b>18,000.00</b></span>
            </div>
        </div>
    </div>

    <!--service category list-->
    <div class="ui segment category-list link cards">
        <div class="ui card service-category">
            <div class="image">
                <img src="/images/logo.png" alt="company-logo">
            </div>
            <div class="content">
                <a class="header">Daniel Louise</a>
            </div>
        </div>
    </div>

    <div class="ui horizontal segments categories-calculator">
        <div class="ui segment service-list">
            <div class="ui menu service-list-header">
                <div class="right menu">
                    <div class="link item">
                        <i class="filter icon __filter_button"></i>
                    </div>
                </div>
            </div>
            <div class="ui three cards link">
                <div class="card service-item">
                    <div class="blurring dimmable image">
                        <!--image ratio is 3:2-->
                        <!--<img src="/images/logo.png">-->
                    </div>
                    <div class="content center aligned">
                        <span class="header">&#8373; <b>18,000.00</b></span>
                        <div class="meta">
                            <span class="date">Create in Sep 2014</span>
                        </div>
                    </div>
                    <div class="ui inverted dimmer">
                        <div class="content">
                            <div class="center">
                                <div class="ui icon buttons small compact">
                                    <button class="ui button service_zoom Zoom__Button"><i class="zoom icon"></i></button>
                                    <button class="ui button service_add"><i class="add to cart icon"></i></button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="ui segment segments service-picked-list">
            <div class="ui segment vertical picked-item-list">
                <div class="ui divided list">
                    <div class="item">
                        <div class="ui grid">
                            <div class="nine wide column">Also quite a lovely city</div>
                            <div class="one wide column remove_button action_button">
                                <span class="ui icon">
                                    <i class="trash icon"></i>
                                </span>
                            </div>
                            <div class="one wide column hide_button action_button">
                                <span class="ui icon">
                                    <i class="hide icon"></i>
                                </span>
                            </div>
                            <div class="five wide column"><span>&#8373; <b>18,000.00</b></span></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="ui segment vertical picked-items-sum grid">
                <div class="ui grid">
                    <div class="eight wide column">Total</div>
                    <div class="three wide column"></div>
                    <div class="five wide column"><span>&#8373; <b>18,000.00</b></span></div>
                </div>
            </div>
        </div>
    </div>
</div>

<!--popups-->

<div class="ui flowing popup hidden  __change_budget_form"
     style="
        min-width: 270px;
        padding: .3em;
        /*border-radius: 0;*/
        height: 40px;
    ">
    <form class="ui form equal mini">
        <div class="fields">
            <div class="field">
                <input type="text" name="new_price" placeholder="Budget...">
            </div>
            <button class="ui button mini">save</button>
            <button class="ui button mini">cancel</button>
        </div>
    </form>

</div>

<div class="ui flowing popup hidden  __filter_form"
    style="
        min-width: 350px;
        padding: .3em;
        /*border-radius: 0;*/
        height: 40px;
    ">
    <form class="ui form equal mini">
        <div class="fields">
            <div class="field">
                <input type="text" name="from_price" placeholder="From">
            </div>
            <div class="field">
                <input type="text" name="to_price" placeholder="To">
            </div>
            <button class="ui button mini" type="submit">filter</button>
        </div>
    </form>

</div>


